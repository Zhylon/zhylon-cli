{
    "name": "zhylon/zhylon-cli",
    "description": "The Zhylon Deployment CLI.",
    "keywords": ["laravel", "zhylon", "cli"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/zhylon/zhylon-cli/issues",
        "source": "https://github.com/zhylon/zhylon-cli"
    },
    "authors": [
        {
            "name": "TobyMaxham",
            "email": "git@maxham.de"
        },
        {
            "name": "Norbert Fox",
            "email": "nfox@zhylon.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "phpseclib/phpseclib": "^3.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.0",
        "laravel/forge-sdk": "^3.12",
        "mockery/mockery": "^1.5",
        "larastan/larastan": "^2.6.0",
        "pestphp/pest": "^1.21",
        "spatie/once": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/zhylon"
    ]
}
